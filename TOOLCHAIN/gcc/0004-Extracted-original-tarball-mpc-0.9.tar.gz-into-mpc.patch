+static void
+reuse_cc_c (mpc_function* function, mpc_srcptr z, mpc_ptr got1, mpc_ptr got2,
+            mpc_ptr exp1, mpc_ptr exp2)
+{
+   known_signs_t ks = {1, 1};
+
+   function->pointer.CC_C (exp1, exp2, z, MPC_RNDNN, MPC_RNDNN);
